{
    "name": "bootstrap5-webpack-boilerplate-2",
    "version": "2.0",
    "description": "A boilerplate using Bootstrap 5, Webpack and Babel, along with jQuery 3, Bootstrap Icons, Handlebars templating, extended favicon support, and updated example pages with dynamically active nav menu items",
    "main": "index.js",
    "scripts": {
        "build": "webpack --config webpack.config.js --mode production",
        "dev": "webpack-dev-server --open --config webpack.config.js --mode development",
        "images": "node ./bin/images.js"
    },
    "type": "module",
    "author": "Jeremy Caris",
    "license": "MIT",
