        "babel-loader": "^9.1.0",
        "bootstrap": "^5.3.2",
        "bootstrap-icons": "^1.11.3",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.7.0",
        "dotenv-webpack": "^8.0.1",
        "fast-glob": "^3.2.12",
        "favicons": "^7.1.5",
